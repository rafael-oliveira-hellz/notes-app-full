{
  "name": "notes-app-mvp",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Rafael de Oliveira (Hellz)",
  "license": "ISC",
  "scripts": {
    "dev": "tsc && node dist/index.js",
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "style:fix": "prettier --write \"src/**/*.ts\"",
    "dev:server": "node ./src/utils/swagger-generator.ts && ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/index.ts -c",
    "start:server": "node dist/index.js",
    "start:queue": "node dist/runner.js",
    "lint": "eslint src --ext ts",
    "postinstall2": "tsc && mv -i -v src/public dist/ && rm -v -rf src",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "tsc": "tsc && cp -r -i -v src/public dist/",
    "swagger": "node ./src/utils/swagger-generator.ts"
  },
  "engines": {
    "node": "19.6.0"
  },
  "dependencies": {
    "@superfaceai/one-sdk": "^2.1.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "imgbb-uploader": "^1.5.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mongodb": "^4.12.1",
    "mongoose": "^6.7.5",
    "mongoose-unique-validator": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.22.0",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^4.1.3",
    "winston": "^3.8.2",
    "winston-mongodb": "^5.1.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.10",
    "@types/node-cron": "^3.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
